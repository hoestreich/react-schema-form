    "react-is": ">=16.13.1"
  },
  "devDependencies": {
    "@mui/icons-material": ">=5.1.0",
    "@mui/material": ">=5.1.0",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
