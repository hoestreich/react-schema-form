{
  "name": "@hoestreich/react-schema-form",
  "version": "0.11.0",
  "description": "React json schema form based on mui v5",
  "keywords": [
    "json schema",
    "schema form",
    "react",
    "react-component",
    "form",
    "react-schema-form",
    "react schema form"
